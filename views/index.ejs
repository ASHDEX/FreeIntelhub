<form action="/search">
  <input name="q" placeholder="Search threats..." />
  <button>Search</button>
</form>

<h3>Vendors</h3>
<ul>
  <% vendors.forEach(v => { %>
    <li><a href="/vendor/<%= encodeURIComponent(v) %>"><%= v %></a></li>
  <% }) %>
</ul>

<h3>Latest</h3>
<ul>
  <% feeds.forEach(f => { %>
    <li>
      <a href="<%= f.link %>" target="_blank"><%= f.title %></a>
      <small>(<%= f.vendor %>)</small>
    </li>
  <% }) %>
</ul>
